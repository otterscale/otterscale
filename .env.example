# Environment Configuration File for OtterScale Application
# This file contains all the necessary environment variables for running the OtterScale bootstrap service
# Copy this file to .env and modify the values according to your deployment environment

# APPLICATION VERSION SECTION
# =============================================================================
# APP_VERSION: Current version of the OtterScale application
#              Used for display purposes and version tracking

APP_VERSION=latest

# =============================================================================
# SERVICE SECTION
# =============================================================================
# SERVICE_PORT: Port number on which the backend API service will run

SERVICE_PORT=8299

# =============================================================================
# WEB APPLICATION SECTION
# =============================================================================
# WEB_PORT: Port number on which the frontend web application will run
# PUBLIC_API_URL: External URL for the API service accessible by the frontend
#                 Must include protocol (http/https) and port number
#                 Replace IP address with your server's actual IP or domain

WEB_PORT=3000
PUBLIC_API_URL=http://localhost:8299
